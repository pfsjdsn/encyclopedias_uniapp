<view class="body"><input class="uni-input common_input" type="text" placeholder="输入需要绑定的邮箱" data-event-opts="{{[['input',[['__set_model',['','email','$event',[]]]]]]}}" value="{{email}}" onInput="__e"/><input class="uni-input common_input" type="text" password="{{true}}" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" onInput="__e"/><button class="{{(('user_set_btn')+' '+((disabled)?'user_set_btn_disable':''))}}" type="primary" disabled="{{disabled}}" loading="{{loading}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" onTap="__e">完成</button></view>