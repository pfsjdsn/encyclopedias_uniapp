<view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" style="{{(scrollStyle)}}" scroll-x="{{true}}"><block a:for="{{tabBars}}" a:for-item="tab" a:for-index="index" a:key="id"><view data-event-opts="{{[['tap',[['tabtap',[index]]]]]}}" class="{{(('swiper-tab-list')+' '+((tabIndex==index)?'active':''))}}" style="{{(scrollItemStyle)}}" onTap="__e">{{''+tab.name+" "+(tab.num?tab.num:'')+''}}<view class="swiper-tab-line"></view></view></block></scroll-view></view>